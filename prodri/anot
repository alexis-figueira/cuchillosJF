sector					tareas							objetivos				agentes

id_sector				id_tarea						id_objetivo				id_agente
nombre					id_sector						id_sector				nombre
jerarquia				id_agente ->0=sector			descripcion				cargo --> 0 empleado 1-->cargo en sector
padre					nombre_tarea					fecha_solicitud			
hijo					descripcion_tarea
						fecha_limite

        
<label for="notas">Ingrese nota</label>
<input type="text" id="notas" name="notas"><br><br>

<label for="objetivos">Ingrese objetivo</label>
<input type="text" id="objetivo" name="objetivo"><br><br>


BDT
organigrama
	org__sectores	-> id_sector = key 
					-> nombre	
					-> jerarquia
					-> padre
					-> estado
	
	org__tareas		-> id_tarea
					-> id_sector
					-> id_agente ( 0 -> tarea para el sector )
					-> nombre_tarea
					-> descripcion_tarea
					-> fecha_solicitud
					-> fecha_programada
					
	objetivos		-> id_objetivo
					-> id_sector
					-> id_agente ( 0 -> objetivo para el sector )
					-> descripcion
					-> fecha_solicitud
					-> fecha_programada
					
	agentes 		-> id_agente
					-> nombre
					-> id_sector
					-> cargo ( o -> empleado / 1 -> cargo en el sector )
